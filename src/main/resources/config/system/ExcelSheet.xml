<excel>
    <sheet name="服务器CPU巡检">
        <table for="cpu">
            <title>
                服务器CPU巡检结果
            </title>
            <row>
                <cell from="name" name="服务器名称" width="20"/>
                <cell from="us" name="CPU使用率us" width="15"/>
                <cell from="id" name="CPU空闲率id" width="15"/>
                <cell name="合并">
                    <cell from="sy" name="sy阿斯达" width="15"/>
                    <cell from="wa" name="wa杀神风" width="20"/>
                </cell>
            </row>
            <extract key="ip"/>
        </table>
    </sheet>
    <sheet name="服务器内存巡检">
        <table for="memory">
            <title>
                服务器物理内存巡检结果
            </title>
            <row>
                <cell from="name" name="服务器名称" width="20"/>
                <cell from="ip" name="IP" width="15"/>
                <cell from="us" name="内存使用率us" width="15"/>
            </row>
            <extract key="ip"/>
        </table>
    </sheet>
    <sheet name="服务器存储巡检">
        <table>
            <title>
                服务器物理存储巡检结果
            </title>
            <row>
                <cell from="name" name="服务器名称" width="20"/>
                <cell from="ip" name="IP" width="15"/>
                <cell from="dir" name="目录" width="15"/>
                <cell from="use" name="使用率" width="15"/>
            </row>
            <extract key="ip" val="dir"/>
        </table>
    </sheet>
    <sheet name="业务量">
        <table for="service-sb">
            <title>
                业务量1
            </title>
            <row>
                <cell from="Host" name="A数量" width="15"/>
                <cell from="Db" name="B数量" width="15"/>
                <cell from="User" name="C数量" width="15"/>
            </row>
        </table>
        <table for="service-rk">
            <title>
                业务量2
            </title>
            <row>
                <cell from="Host" name="D数量" width="15"/>
                <cell from="Db" name="E数量" width="15"/>
                <cell from="User" name="F数量" width="15"/>
                <cell name="G数量">
                    <cell from="g1" name="G1数量" width="15"/>
                    <cell from="g2" name="G2数量" width="15"/>
                </cell>
                <cell name="M数量">
                    <cell from="m1" name="m1数量" width="15"/>
                    <cell from="m2" name="m2数量" width="15"/>
                </cell>
            </row>
        </table>
        <table for="service-zt">
            <title>
                业务量3
            </title>
            <row>
                <cell from="Host" name="H数量" width="15"/>
                <cell from="Db" name="I数量" width="15"/>
                <cell from="User" name="J数量" width="15"/>
                <cell from="K" name="K数量" width="15"/>
                <cell from="L" name="L数量" width="15"/>
            </row>
        </table>
    </sheet>
</excel>