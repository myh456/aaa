<root>
    <cpu id="1">
        <threshold>
            <us higher="80"/>
            <id lower="20"/>
        </threshold>
    </cpu>
    <cpu id="2">
        <threshold>
            <us higher="90"/>
            <id lower="10"/>
        </threshold>
    </cpu>

    <memory id="1">
        <threshold>
            <mem_used higher="80"/>
            <free_used higher="10"/>
        </threshold>
    </memory>
    <memory id="2">
        <threshold>
            <mem_used higher="85"/>
            <free_used higher="10"/>
        </threshold>
    </memory>

    <storage id="1">
        <path-attr>/ /home /opt</path-attr>
        <threshold>
            <use_root higher="80"/>
            <use_home higher="80"/>
        </threshold>
    </storage>

    <weblogic-process id="1"/>
    <weblogic-service id="1">
        <attrs>
            <admin>admin</admin>
            <password>admin123</password>
        </attrs>
        <attrs>
            <admin>root</admin>
            <password>root</password>
        </attrs>
        <threshold>
            <exclusive higher="5"/>
        </threshold>
    </weblogic-service>

    <expire id="1">
        <threshold>
            <remaining_days lower="7"/>
        </threshold>
    </expire>

    <instance id="1"/>
    <expire id="1"/>
    <tablespace id="1"/>
    <asm id="1"/>

    <service-zt id="1">
        <db-attr>mysql.db</db-attr>
        <threshold>
            <failed higher="6"/>
        </threshold>
    </service-zt>
    <service-zt id="2">
        <db-attr>mysql.db</db-attr>
        <db-attr>mysql.user</db-attr>
        <threshold>
            <failed higher="6"/>
        </threshold>
    </service-zt>
    <service-sb id="1">
        <threshold>
            <failed higher="6"/>
        </threshold>
    </service-sb>
    <service-rkls id="1">
        <threshold>
            <stream lower="1000"/>
        </threshold>
    </service-rkls>
 </root>